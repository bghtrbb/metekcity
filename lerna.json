{
  "packages": [
    "packages/*"
  ],
  "version": "1.1.2",
  "npmClient": "yarn",
  "useWorkspaces": true,
  "command": {
    "publish": {
      "message": "chore(release): publish [skip ci]",
      "registry": "https://registry.npmjs.org"
    },
    "bootstrap": {
      "npmClientArgs": [
        "--frozen-lockfile",
        "--check-files",
        "--prefer-offline"
      ]
    }
  }
}
